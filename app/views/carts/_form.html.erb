<%= form_for @cart do |f| %>
    <div class="container">
        <div class="input-field col s6">
            <%= f.label "User: " %><br>
            <%= f.collection_select(:user_id, User.all, :id, :username) %><br><br>
        </div>
        <div class="input-field col s6">
            <%= f.label "Artwork: " %><br>
            <%= f.collection_select(:artwork_id, Artwork.all, :id, :name) %><br><br>
        </div>
        <%= f.submit "Add to Cart"%>
    </div>
<% end%>