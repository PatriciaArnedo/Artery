<%= form_for @artwork do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :medium %><br>
<%= f.text_field :medium %><br>
<%= f.label :size %><br>
<%= f.text_field :size %><br>
<%= f.label "Artist" %><br>
<%= f.collection_select(Artist.all, :artist_id, :id, :name) %><br>
<%= f.submit %>
<% end%>