<strong>Username: </strong> <%= @user.username %><br>

<h3>Items in my cart:
<ul>
<% @user.carts.each do |cart| %>
    <li><%= link_to cart.artwork.name, artwork_path(cart.artwork)%> <%= button_to "Remove", cart_path(cart), method: :delete %><br>
<%end %>
</ul>

<p><%= link_to "Users", users_path %></p>
<p><%= link_to "Add Items to My Cart", new_cart_path %></p>


